<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<documentburster>
  <settings>
    <version>8.8.4</version>
    <template>99-coi-cert-email3</template>
    <burstfilename>${burst_token}.${input_document_extension}</burstfilename>
    <mergefilename>merged.pdf</mergefilename>
    <outputfolder>output/${input_document_name}/${now?string["yyyy.MM.dd_HH.mm.ss.SSS"]}</outputfolder>
    <backupfolder>backup/${input_document_name}/${now?string["yyyy.MM.dd_HH.mm.ss.SSS"]}</backupfolder>
    <quarantinefiles>true</quarantinefiles>
    <quarantinefolder>quarantine/${input_document_name}/${now?string["yyyy.MM.dd_HH.mm.ss.SSS"]}</quarantinefolder>
    <logsarchivesfolder>logs/archives/${input_document_name}/${now?string["yyyy.MM.dd_HH.mm.ss.SSS"]}</logsarchivesfolder>
    <statsfilename>_stats-${stats_info}.log</statsfilename>
    <sendfiles>
      <email>true</email>
      <upload>false</upload>
      <web>false</web>
      <sms>false</sms>
    </sendfiles>
    <deletefiles>false</deletefiles>
    <emailserver>
      <host></host>
      <port>25</port>
      <userid/>
      <userpassword/>
      <usessl>false</usessl>
      <usetls>false</usetls>
      <fromaddress>Leidos-Ethics-and-Compliance@leidos.com</fromaddress>
      <name>Leidos Ethics and Compliance</name>
    </emailserver>
    <htmlemail>true</htmlemail>
    <htmlemaileditcode>true</htmlemaileditcode>
    <emailsettings>
      <to>${var0}</to>
      <cc>${var1}</cc>
      <bcc/>
      <subject>Requirement Outstanding: Conflicts of Interest Certification Status Report</subject>
      <text/>
      <html>&lt;img src="images/bla.jpg"&gt;</html>
    </emailsettings>
    <attachments>
      <items>
        <attachment path="${extracted_file_path}" order="0"/>
      </items>
      <archive>
        <archiveattachments>false</archiveattachments>
        <archivefilename>reports-${burst_token}.zip</archivefilename>
      </archive>
    </attachments>
    <emailrfc2822validator>
      <allowquotedidentifiers>true</allowquotedidentifiers>
      <allowparensinlocalpart>true</allowparensinlocalpart>
      <allowdomainliterals/>
      <allowdotinatext/>
      <allowsquarebracketsinatext/>
      <skipvalidationfor/>
    </emailrfc2822validator>
    <simplejavamail>
      <active/>
      <replytoname/>
      <replytoaddress/>
      <bouncetoname/>
      <bouncetoaddress/>
      <receipttoname/>
      <receipttoaddress/>
      <dispositionnotificationtoname/>
      <dispositionnotificationtoaddress/>
      <customemailheaders/>
      <customsessionproperties/>
      <javaxmaildebug/>
      <transportmodeloggingonly/>
      <proxy>
        <host/>
        <port>1080</port>
        <username/>
        <password/>
        <socks5bridgeport>1081</socks5bridgeport>
      </proxy>
      <async/>
      <poolsize/>
      <poolsizekeepalivetime/>
      <connectionpool>
        <clusterkeyuuid/>
        <coresize/>
        <maxsize/>
        <claimtimeoutmillis/>
        <expireaftermillis/>
        <loadbalancingstrategy/>
      </connectionpool>
      <sessiontimeoutmillis/>
      <trustallhosts/>
      <trustedhosts/>
      <verifyserveridentity/>
      <opportunistictls/>
      <dkim>
        <privatekey/>
        <signingdomain/>
        <selector/>
      </dkim>
      <smime>
        <signingkeystore/>
        <signingkeystorepassword/>
        <signingkeyalias/>
        <signingkeypassword/>
        <encryptioncertificate/>
      </smime>
    </simplejavamail>
    <uploadsettings>
      <ftpcommand/>
      <filesharecommand/>
      <ftpscommand/>
      <sftpcommand/>
      <httpcommand/>
      <cloudcommand/>
    </uploadsettings>
    <webuploadsettings>
      <documentbursterwebcommand/>
      <mssharepointcommand/>
      <wordpresscommand/>
      <drupalcommand/>
      <joomlacommand/>
      <otherwebcommand/>
    </webuploadsettings>
    <smssettings>
      <twilio>
        <accountsid/>
        <authtoken/>
      </twilio>
      <fromtelephonenumber/>
      <totelephonenumber/>
      <text/>
    </smssettings>
    <qualityassurance>
      <emailserver>
        <host>localhost</host>
        <port>1025</port>
        <userid/>
        <userpassword/>
        <usessl>false</usessl>
        <usetls>false</usetls>
        <fromaddress>from@emailaddress.com</fromaddress>
        <name>From Name</name>
        <weburl>http://localhost:8025</weburl>
      </emailserver>
    </qualityassurance>
    <split2ndtime>false</split2ndtime>
    <bursttokendelimiters>
      <start>{</start>
      <end>}</end>
      <start2nd>[</start2nd>
      <end2nd>]</end2nd>
    </bursttokendelimiters>
    <numberofuservariables>100</numberofuservariables>
    <delayeachdistributionby>0</delayeachdistributionby>
    <reusetokenswhennotfound>false</reusetokenswhennotfound>
    <failjobifanydistributionfails>true</failjobifanydistributionfails>
    <enableretrypolicy>false</enableretrypolicy>
    <retrypolicy>
      <delay>3</delay>
      <maxdelay>30</maxdelay>
      <maxretries>3</maxretries>
    </retrypolicy>
    <locale>
      <language/>
      <country/>
    </locale>
    <sortbyposition>false</sortbyposition>
    <suppressduplicateoverlappingtext>false</suppressduplicateoverlappingtext>
    <shouldseparatebybeads>true</shouldseparatebybeads>
    <averagechartolerancevalue/>
    <spacingtolerancevalue/>
    <enableincubatingfeatures>false</enableincubatingfeatures>
    <updatesettings>
      <copylicensinginformation>true</copylicensinginformation>
      <copyoutputfiles>false</copyoutputfiles>
      <copylogfiles>true</copylogfiles>
      <copyquarantinefiles>false</copyquarantinefiles>
      <copybackupfiles>false</copybackupfiles>
    </updatesettings>
  </settings>
</documentburster>