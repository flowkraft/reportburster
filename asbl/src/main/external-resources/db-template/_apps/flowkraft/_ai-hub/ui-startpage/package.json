{
  "name": "flowkraft-ai-hub-frend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "db:push": "npx drizzle-kit push",
    "db:studio": "npx drizzle-kit studio",
    "build:agents": "npx --yes -p typescript tsc scripts/init-agents.ts scripts/delete-agents.ts scripts/list-agents.ts scripts/truncate-db.ts --outDir .build --module commonjs --target es2020",
    "agents:list": "npm run build:agents && node ./.build/scripts/list-agents.js",
    "agents:provision": "npm run build:agents && node ./.build/scripts/init-agents.js",
    "agents:provision-force": "npm run build:agents && FORCE_PROVISION=true node ./.build/scripts/init-agents.js",
    "agents:delete": "npm run build:agents && node ./.build/scripts/delete-agents.js",
    "agents:truncate-empty-db:dry-run": "npm run build:agents && node ./.build/scripts/truncate-db.js",
    "agents:truncate-empty-db:now": "npm run build:agents && TRUNCATE_YES=true node ./.build/scripts/truncate-db.js",
    "agents:truncate-empty-db:force": "npm run build:agents && TRUNCATE_YES=true FORCE_TRUNCATE=true node ./.build/scripts/truncate-db.js"
  },
  "dependencies": {
    "@ai-sdk/react": "^2.0.118",
    "@letta-ai/letta-client": "^1.6.1",
    "@letta-ai/vercel-ai-sdk-provider": "^1.4.0",
    "ai": "^5.0.116",
    "matrix-js-sdk": "^37.1.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slot": "^1.1.1",
    "better-sqlite3": "^12.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "lucide-react": "^0.468.0",
    "next": "16.1.6",
    "pako": "^2.1.0",
    "prism-react-renderer": "^2.4.1",
    "prismjs": "^1.30.0",
    "react": "19.0.1",
    "react-dom": "19.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.8",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22",
    "@types/pako": "^2.0.3",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "postcss": "^8",
    "tailwindcss": "4.1.8",
    "typescript": "^5"
  }
}
