<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"

	xmlns:util="http://www.springframework.org/schema/util" xmlns:context="http://www.springframework.org/schema/context"

	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd

		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.0.xsd

		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<bean id="uploadScheduled" class="com.sourcekraft.batch.admin.web.controller.ScheduledController">
		<property name="fileService" ref="scheduledFileService" />
	</bean>
	
	<bean id="loginController" class="com.sourcekraft.batch.admin.web.controller.LoginController"/>

</beans>

