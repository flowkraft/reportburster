{
  "workbench.colorCustomizations": {
    "activityBar.background": "#B22222",
    "activityBar.foreground": "#ffffff",
    "activityBar.inactiveForeground": "#000000"
  },
  "window.title": "${dirty} ReportBurster (Kate) > ${activeEditorShort}",
  "files.watcherExclude": {
    "**/node_modules/*/**": true,
    "**/.venv/*/**": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/CVS": true
  },
  "hide-files.files": [],
  "java.compile.nullAnalysis.mode": "disabled",
  "sqltools.connections": [
    {
      "previewLimit": 50,
      "driver": "SQLite",
      "database": "${workspaceFolder:reportburster}/frend/reporting/testground/e2e/db/sample-northwind-sqlite/northwind.db",
      "name": "Northwind"
    }
  ],
  "sqltools.useNodeRuntime": true
}
